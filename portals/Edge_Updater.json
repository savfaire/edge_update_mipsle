                        "outgoing_parser": {
                            "isDebugOn": false,
                            "value": "if (!!datasources.selected_edge.latestData() \u0026\u0026 \n    datasources.selected_edge.latestData().name != \"\") {\n\n    datasources.edge_update_logs_topic.sendData(\"\");\n    datasources.edge_update_response_topic.sendData({});\n\n    datasources.edge_update_request_topic.settings({ ...datasources.edge_update_request_topic.settings(), TOPIC_NAME: \"edge/update/\" + datasources.selected_edge.latestData().name + \"/request\"});\n    datasources.edge_update_response_topic.settings({ ...datasources.edge_update_response_topic.settings(), TOPIC_NAME: \"edge/update/\" + datasources.selected_edge.latestData().name + \"/response\"});\n    datasources.edge_update_logs_topic.settings({ ...datasources.edge_update_logs_topic.settings(), TOPIC_NAME: \"edge/update/\" + datasources.selected_edge.latestData().name + \"/logs\"});\n\n    //Hide the upgrade edge button\n    $('#upgrade_edge_btn').hide();\n\n    CB_PORTAL.Loader.show(\"edge_update_logs\");\n\n    return JSON.stringify({\n        \"version\": datasources.portal_getPlatformVersion.latestData().results\n    });\n}\n\n"

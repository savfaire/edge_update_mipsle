                    "textContent": {
                        "dataType": "DYNAMIC_DATA_TYPE",
                        "id": "f09a93eb-351d-4294-84f7-df1f3816c864",
                        "incoming_parser": {
                            "isDebugOn": false,
                            "value": "var logsTxt = \"\";\nvar theLogs;\n\ntry {\n    theLogs = JSON.parse(this.datasource.edge_update_logs_topic);\n} catch (e) {\n    theLogs = {\"logs\":[]};\n}\n\nif (theLogs.logs) {\n    for(var i=0; i\u003ctheLogs.logs.length; i++) {\n        logsTxt += theLogs.logs[i];\n    }\n}\n\nif (this.datasource.edge_update_response_topic) {\n    var resp = JSON.parse(this.datasource.edge_update_response_topic);\n\n    if (resp.hasOwnProperty('success')) {\n        if (resp.success) {\n            logsTxt += \"Edge updated successfully to version \" + resp.version;\n\n            //Hide the upgrade edge button\n            $('#upgrade_edge_btn').hide();\n\n            //Refresh the edge_list datasource\n            datasources.edge_list.refresh();\n\n            setTimeout(function(){\n                var theEdge = datasources.edge_list.latestData().find(function(edge){\n                    return edge.name === datasources.selected_edge.latestData().name;\n                });\n\n                //Update the selected Edge datasource\n                datasources.selected_edge.sendData(theEdge); \n            }, 500);\n        } else {\n            logsTxt += \"An error occurred while updating Edge: \" + resp.error;\n        \n            //Show the upgrade edge button\n            $('#upgrade_edge_btn').show();\n        }\n        CB_PORTAL.Loader.hide(\"edge_update_logs\");\n    }\n}\n\nreturn logsTxt;"
                        }
                    }
                },
